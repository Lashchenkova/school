{% extends 'AppBundle::admin_base.html.twig' %}

{% block title %}Students{% endblock %}

{% block stylesheets %}
    {% stylesheets 'bundles/app/css/students.css' filter='cssrewrite' %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}

{% block body %}
    {% for student in students %}
        {{ student.firstname }} <br>
        {{ student.lastname }} <br>
        {{ student.dateofbirth|date('Y-m-d') }} <br>
    {% endfor %}
{% endblock %}

{% block javascripts %}{% endblock %}